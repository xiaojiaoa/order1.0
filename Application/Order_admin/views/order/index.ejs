<!-- MAIN PANEL -->
<div id="main" role="main">

    <!-- RIBBON -->
    <div id="ribbon">

        <!-- breadcrumb -->
        <ol class="breadcrumb">
            <li>后台管理</li>
            <li>基础页面</li>
        </ol>
        <!-- end breadcrumb -->

        <!-- You can also add more buttons to the
        ribbon for further usability

        Example below:

        <span class="ribbon-button-alignment pull-right">
        <span id="search" class="btn btn-ribbon hidden-xs" data-title="search"><i class="fa-grid"></i> Change Grid</span>
        <span id="add" class="btn btn-ribbon hidden-xs" data-title="add"><i class="fa-plus"></i> Add</span>
        <span id="search" class="btn btn-ribbon" data-title="search"><i class="fa-search"></i> <span class="hidden-mobile">Search</span></span>
        </span> -->

    </div>
    <!-- END RIBBON -->


    <!-- MAIN CONTENT -->
    <div id="content">


        <!--
            The ID "widget-grid" will start to initialize all widgets below
            You do not need to use widgets if you dont want to. Simply remove
            the <section></section> and you can use wells or panels instead
            -->


        <!-- well -->
        <div class="well">
            <!-- row -->
            <div class="row">
                <!-- col -->
                <div class="col-xs-12">
                    <!--<legend>订单搜索</legend>-->
                    <!-- row -->
                    <div class="row search-form">
                        <form class="form-horizontal">

                            <div class="col-xs-12">

                                <!--输入字段-->
                                <div class="col-xs-3">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label">字段名:</label>
                                        <div class="col-xs-8">
                                            <input class="form-control" placeholder="提示信息" type="text" value="默认值"
                                                   name="">
                                        </div>
                                    </div>
                                </div>

                                <!--下拉选择 有键值-->
                                <div class="col-xs-3">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label">下拉选项:</label>
                                        <div class="col-xs-8">
                                            <select class="form-control" name="">
                                                <option>- 请选择 -</option>
                                                <option value="1">选项1</option>
                                                <option value="2">选项2</option>
                                            </select>

                                        </div>
                                    </div>
                                </div>

                                <!--下拉选择 无键值-->
                                <div class="col-xs-3">
                                    <div class="form-group">
                                        <div class="col-xs-12">
                                            <select class="form-control" name="">
                                                <option>- 请选择 -</option>
                                                <option value="1">选项1</option>
                                                <option value="2">选项2</option>
                                            </select>

                                        </div>
                                    </div>
                                </div>

                                <!--下拉选择+输入组合-->
                                <div class="col-xs-3">
                                    <div class="form-group">
                                        <div class="col-xs-5 tight-right">
                                            <select class="form-control" name="">
                                                <option>- 请选择 -</option>
                                                <option value="1">选项1</option>
                                                <option value="2">选项2</option>
                                            </select>
                                        </div>
                                        <div class="col-xs-7 tight-left">
                                            <input class="form-control" placeholder="提示信息" type="text" value="默认值"
                                                   name="">
                                        </div>
                                    </div>
                                </div>


                            </div>
                            <div class="col-xs-12">
                                <!--下拉选择 无键值-->
                                <div class="col-xs-3">
                                    <div class="form-group">
                                        <div class="col-xs-12">
                                            <select class="select2" name="">
                                                <option>- 带搜索的选择 请选择 -</option>
                                                <option value="1">选项1</option>
                                                <option value="2">选项2</option>
                                                <option value="3">选项3</option>
                                            </select>

                                        </div>
                                    </div>
                                </div>


                                <!--下拉选择 无键值-->
                                <div class="col-xs-4">
                                    <div class="form-group">
                                        <label class="col-xs-3 control-label">范围选择:</label>
                                        <div class="col-xs-4 tight-right">
                                            <select class="form-control" name="">
                                                <option>- 起始 请选择 -</option>
                                                <option value="1">选项1</option>
                                                <option value="2">选项2</option>
                                                <option value="3">选项3</option>
                                            </select>
                                        </div>
                                        <label class="col-xs-1 control-label "> 至 </label>
                                        <div class="col-xs-4 ">
                                            <select class="form-control" name="">
                                                <option>- 截止 请选择 -</option>
                                                <option value="1">选项1</option>
                                                <option value="2">选项2</option>
                                                <option value="3">选项3</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>

                                <!--键值+下拉选择+输入组合-->
                                <div class="col-xs-5">
                                    <div class="form-group">
                                        <label class="col-xs-3 control-label "> 请选择: </label>
                                        <div class="col-xs-4 tight-right">
                                            <select class="form-control" name="">
                                                <option>- 请选择 -</option>
                                                <option value="1">选项1</option>
                                                <option value="2">选项2</option>
                                            </select>
                                        </div>
                                        <div class="col-xs-5 tight-left">
                                            <input class="form-control" placeholder="提示信息" type="text" value="默认值"
                                                   name="">
                                        </div>
                                    </div>
                                </div>


                            </div>

                            <div class="col-xs-12">
                                <hr class="simple">
                                <div class="col-xs-12">
                                    <button class="btn btn-primary " type="submit">
                                        <i class="fa fa-save"></i>
                                        确认
                                    </button>
                                </div>
                            </div>

                        </form>
                    </div>
                    <!-- end row -->
                </div>
                <!-- end col -->
            </div>
            <!-- end row -->
        </div>
        <!-- end well -->

        <div class="well">

