<%- include('../layouts/head_top'); %>
<%- include('../layouts/head_bottom'); %>
<%- include('../layouts/header'); %>
<%- include('../layouts/nav'); %>

<!-- MAIN PANEL -->
<div id="main" role="main">

    <!-- RIBBON -->
    <div id="ribbon">

        <!-- breadcrumb -->
        <ol class="breadcrumb">
            <li>后台管理</li>
            <li>货位查看</li>
        </ol>
        <!-- end breadcrumb -->


    </div>
    <!-- END RIBBON -->


    <!-- MAIN CONTENT -->
    <div id="content">
        <!-- well -->
        <div class="well">
            <!-- row -->
            <div class="row">
                <!-- col -->
                <div class="col-xs-12">
                    <!--<legend>订单搜索</legend>-->
                    <!-- row -->
                    <div class="row search-form">
                        <form class="form-horizontal" method="get" action="/cargospace" id="create-store">

                            <div class="col-xs-12">

                                <!--输入字段-->
                                <div class="col-xs-4">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label"><span class="requird"></span> 工厂:</label>
                                        <div class="col-xs-8">
                                            <select class="form-control validate[required]" name="ftyId" id="ftyId">
                                                <option value="">- 请选择 -</option>
                                                <% factoryList.forEach(function (element) { %>
                                                <option value="<%= element.ftyId %>"  >
                                                    <%= element.name %>
                                                </option>
                                                <% }) %>
                                            </select>
                                        </div>
                                    </div>
                                </div>

                                <!--输入字段-->
                                <div class="col-xs-4">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label"><span class="requird"></span> 仓库:</label>
                                        <div class="col-xs-8">
                                            <select class="form-control validate[required]" name="whseId" id="whseId">
                                                <option value="">- 请选择 -</option>

                                            </select>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-xs-4">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label"><span class="requird"></span> 仓库区域:</label>
                                        <div class="col-xs-8">
                                            <select class="form-control validate[required]" name="regionId" id="regionId">
                                                <option value="">- 请选择 -</option>

                                            </select>
                                        </div>
                                    </div>
                                </div>


                            </div>

                            <div class="col-xs-12">

                                <!--输入字段-->
                                <div class="col-xs-4">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label">货位号:</label>
                                        <div class="col-xs-8">
                                            <input class="form-control"  type="text"
                                                   value=""
                                                   name="spaceId">
                                        </div>
                                    </div>
                                </div>

                                <!--输入字段-->
                                <div class="col-xs-4">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label">显示部分:</label>
                                        <div class="col-xs-8">
                                            <div class="col-xs-6 no-padding">
                                                <div class="col-xs-8 no-padding">
                                                    <input class="form-control validate[custom[onlyNumberSp]]" placeholder="行" type="text"
                                                           name="spaceRow" value="">
                                                </div>
                                                <label class="control-label ">行 </label>
                                            </div>
                                            <div class="col-xs-6 no-padding">
                                                <div class="col-xs-8 no-padding">
                                                    <input class="form-control validate[custom[onlyNumberSp]]" placeholder="列" type="text"
                                                           name="spaceColumn" value="">
                                                </div>
                                                <label class="control-label ">列 </label>
                                            </div>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-xs-4">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label">显示方式:</label>
                                        <div class="col-xs-8">
                                            <select class="form-control" name="isAll">
                                                <option value="2" >不显示已排满</option>
                                                <option value="1" >显示所有</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-xs-12">
                                <hr class="simple">
                                <div class="col-xs-12 text-center">
                                    <% if (DWY_Helper.hasPermission(Permission.goodsAllocation.create.id, permission) ){ %>
                                    <a class="btn btn-primary margin-right-5" href="/cargospace/create">
                                        <i class="fa fa-save"></i>
                                        新建货位
                                    </a>
                                    <% } %>
                                    <button class="btn btn-primary " type="submit">
                                        <i class="fa fa-search"></i>
                                        查询
                                    </button>
                                </div>
                            </div>

                        </form>
                    </div>
                    <!-- end row -->
                </div>
                <!-- end col -->
            </div>
            <!-- end row -->
        </div>
        <!-- end well -->

        <div class="well">

            <!-- row -->
            <div class="row">
                <div class="col-xs-12">
                    <h6>货位列表</h6>

                    <hr class="simple">
                </div>
            </div>

            <!-- row -->
            <div class="row">
                <div class="col-xs-12">
                    <div class="table-responsive">
                        <table class="table table-bordered table-striped table-hover">
                            <thead>
                            <tr>
                                <th>序号</th>
                                <th>货位号</th>
                                <th>所在行/列/层</th>
                                <th>最大承重</th>
                                <th>最大存货数量</th>
                                <th>货架长宽高</th>
                                <th>货位状态</th>
                            </tr>
                            </thead>
                            <tbody>
                            <tr>
                                <td colspan="7" align="center">暂无结果</td>
                            </tr>
                            </tbody>
                        </table>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- END MAIN CONTENT -->

</div>
<!-- END MAIN PANEL -->

<%- include('../layouts/footer'); %>
<%- include('../layouts/footer_common_script'); %>

<%- include('./js_function'); %>

<script type="text/javascript">

    $(document).ready(function () {
        pageSetUp();
        $("#create-store").validationEngine({scroll: false});


    })

</script>


<%- include('../layouts/foot'); %>
