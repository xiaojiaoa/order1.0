<script type="text/javascript">
        function checkFuc(a, b) {
            //点击全选/全不选影响复选框状态
            $(a).click(function () {
                $(b).prop("checked", this.checked);
            });
            //复选框是否选中影响全选/全不选框状态-复选框的总数是否与选中的复选框数量相等
            $(b).click(function () {
                var $tmp = $(b);
                $(a).prop("checked", $tmp.length == $tmp.filter(":checked").length);
            });
        }

        //获取选中的checkbox的值
        function getList(a, b) {
            var myArray = new Array();
            var sendType;
            switch (a) {
                case "#checkAll01":
                    sendType = 1;
                    break;
                case "#checkAll02":
                    sendType = 2;
                    break;
                case "#checkAll03":
                    sendType = 3;
                    break;
                case "#checkAll04":
                    sendType = 1;
                    break;
                case "#checkAll05":
                    sendType = 2;
                    break;
                case "#checkAll06":
                    sendType = 3;
                    break;
            }
            if ($(a).is(":checked")) {
                var sendValue = -1;
                var a = new Object();
                a = {
                    sendType: sendType,
                    sendValue: sendValue,
                }
                myArray.push(a);
            } else {
                $(b).each(function () {
                    var sendValue = $(this).val();
                    var a = new Object();
                    a = {
                        sendType: sendType,
                        sendValue: sendValue,
                    }

                    myArray.push(a);
                });
            }
            return myArray;
        }

        //设置选中的checkbox的值
       function setList(data){

           //初始化复选框--均为未选中状态
           $("#checkAll04").prop("checked", false);
           $("input[name=storeTypeM]:checkbox").prop("checked",false);
           $("#checkAll05").prop("checked",false);
           $("input[name=addrTypeM]:checkbox").prop("checked",false);
           $("#checkAll06").prop("checked",false);
           $("input[name=organizationTypeM]:checkbox").prop("checked",false);

           //根据后台传过来的值设置checkbox的状态
           for(var i in data){
               if(data[i].sendValue==-1){
                   switch (data[i].sendType) {
                       case 1:
                           $("#checkAll04").prop("checked", true);
                           $("input[name=storeTypeM]:checkbox").prop("checked",true);
                           break;
                       case 2:
                           $("#checkAll05").prop("checked",true);
                           $("input[name=addrTypeM]:checkbox").prop("checked",true);
                           break;
                       case 3:
                           $("#checkAll06").prop("checked",true);
                           $("input[name=organizationTypeM]:checkbox").prop("checked",true);
                           break;
                   }
               }
               else {
                   switch (data[i].sendType) {
                       case 1:
                           $("input[name=storeTypeM]:checkbox").each(function(){
                               if(data[i].sendValue==$(this).val()){
                                   $(this).prop("checked",true);
                               }
                           });
                           break;
                       case 2:
                           $("input[name=addrTypeM]:checkbox").each(function(){
                               if(data[i].sendValue==$(this).val()){
                                   $(this).prop("checked",true);
                               }
                           });
                           break;
                       case 3:
                           $("input[name=organizationTypeM]:checkbox").each(function(){
                               if(data[i].sendValue==$(this).val()){
                                   $(this).prop("checked",true);
                               }
                           });
                           break;
                   }
               }
           }
       }

</script>