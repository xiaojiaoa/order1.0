<%- include('../layouts/head_top'); %>
<%- include('../layouts/head_bottom'); %>
<%- include('../layouts/header'); %>
<%- include('../layouts/nav'); %>


<!-- MAIN PANEL -->
<div id="main" role="main">

    <!-- RIBBON -->
    <div id="ribbon">

        <!-- breadcrumb -->
        <ol class="breadcrumb">
            <li>后台管理</li>
            <li>新建空间</li>
        </ol>
        <!-- end breadcrumb -->

        <!-- You can also add more buttons to the
        ribbon for further usability

        Example below:

        <span class="ribbon-button-alignment pull-right">
        <span id="search" class="btn btn-ribbon hidden-xs" data-title="search"><i class="fa-grid"></i> Change Grid</span>
        <span id="add" class="btn btn-ribbon hidden-xs" data-title="add"><i class="fa-plus"></i> Add</span>
        <span id="search" class="btn btn-ribbon" data-title="search"><i class="fa-search"></i> <span class="hidden-mobile">Search</span></span>
        </span> -->

    </div>
    <!-- END RIBBON -->


    <!-- MAIN CONTENT -->
    <div id="content">


        <!--
            The ID "widget-grid" will start to initialize all widgets below
            You do not need to use widgets if you dont want to. Simply remove
            the <section></section> and you can use wells or panels instead
            -->


        <!-- well -->
        <div class="well">
            <!-- row -->
            <div class="row">
                <!-- col -->
                <div class="col-xs-12">
                    <legend>新增空间</legend>

                    <!-- row -->
                    <div class="row search-form">
                        <form class="form-horizontal" method="post" action="/taskseq/space">

                            <div class="col-xs-12">

                                <!--键值+下拉选择+输入组合-->
                                <div class="col-xs-4">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label "> 客户号: </label>
                                        <div class="col-xs-5 tight-right">
                                            <input class="form-control" placeholder="客户号" type="cid"
                                                   name="cid" value="<%= custInfo.cid %>">
                                        </div>
                                    </div>
                                </div>

                                <!--键值+下拉选择+输入组合-->
                                <div class="col-xs-4">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label "> 客户姓名: </label>
                                        <div class="col-xs-5 tight-right">
                                            <input class="form-control" placeholder="客户姓名" type="custName"
                                                   name="custName" value=<%= custInfo.custName %>>
                                        </div>
                                    </div>
                                </div>

                            </div>

                            <div class="col-xs-12">

                                <!--键值+下拉选择+输入组合-->
                                <div class="col-xs-4">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label "> 客户电话: </label>
                                        <div class="col-xs-5 tight-right">
                                            <input class="form-control" placeholder="客户电话" type="mobile"
                                                   name="mobile" value=<%= custInfo.custMobile%>>
                                        </div>
                                    </div>
                                </div>

                            </div>

                            <!--输入字段-->
                            <div class="col-xs-12">

                                <!--键值+下拉选择+输入组合-->
                                <div class="col-xs-4">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label "> 空间类型: </label>
                                        <div class="col-xs-4 tight-left">
                                            <select class="form-control" >
                                                <option>- 空间类型 -</option>
                                                <option value="1">男</option>
                                                <option value="2">女</option>
                                            </select>
                                        </div>
                                        <div class="col-xs-4 tight-left">
                                            <select class="form-control" name="spaceId">
                                                <option>- 空间类型 -</option>
                                                <option value="1">男</option>
                                                <option value="2">女</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>

                                <!--键值+下拉选择+输入组合-->
                                <div class="col-xs-4">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label "> 空间名: </label>
                                        <div class="col-xs-5 tight-right">
                                            <input class="form-control" placeholder="空间名" type="spaceName"
                                                   name="spaceName">
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-xs-8">
                                <hr class="simple">
                                <div class="col-xs-12 text-center">
                                    <button class="btn btn-primary " type="submit">
                                        <i class="fa fa-save"></i>
                                         提交
                                    </button>
                                </div>
                            </div>

                        </form>
                    </div>
                    <!-- end row -->

                </div>
                <!-- end col -->
            </div>
            <!-- end row -->
        </div>
        <!-- end well -->


    </div>
    <!-- END MAIN CONTENT -->

</div>
<!-- END MAIN PANEL -->

<%- include('../layouts/footer'); %>
<%- include('../layouts/footer_common_script'); %>


