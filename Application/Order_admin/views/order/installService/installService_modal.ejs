<form id="assignInstallGroup" class="col-xs-12 layer-template" action="/installServiceTask" method="post">
    <div class="col-xs-12">
        <div class="col-xs-2 margin-top-10 text-center">
            安装组名称:
        </div>
        <div class="col-xs-6">
            <select class="form-control validate[required]" name="did">
                <option value="">--请选择--</option>
                <%
                if(installGroup){
                installGroup.forEach(function (element,index) {%>
                <option value="<%= element.did %>" class="selectInstallGroup"><%= element.bname %>—<%= element.dname %></option>
                <%})}%>
            </select>
            <input type="hidden" value="" name="tid">
        </div>
    </div>
    <div class="col-xs-12 text-center margin-top-10">
        <button class="btn btn-primary margin-top-10" type="submit">确认指定</button>
    </div>
</form>

<script type="text/javascript">
    $(document).ready(function () {
        pageSetUp();
        $(".fg-date-picker").flatpickr(data_time_picker.data_picker); // jQuery
        $("#assignInstallGroup").validationEngine({scroll: false});
    })
</script>