<%- include('../layouts/head_top'); %>
<%- include('../layouts/head_bottom'); %>
<%- include('../layouts/header'); %>
<%- include('../layouts/nav'); %>


<!-- MAIN PANEL -->
<div id="main" role="main">
    <!-- RIBBON -->
    <div id="ribbon">
        <!-- breadcrumb -->
        <ol class="breadcrumb">
            <li>后台管理</li>
            <li>待安装列表</li>
        </ol>
        <!-- end breadcrumb -->
    </div>
    <!-- END RIBBON -->
    <!-- MAIN CONTENT -->
    <div id="content">
        <div class="well">
            <div class="row">
                <div class="col-xs-12">
                    <!--<legend>客户搜索</legend>-->
                    <div class="row search-form">
                        <form class="form-horizontal" method="get">
                            <div class="col-xs-12">
                                <div class="col-xs-3">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label">订单编号:</label>
                                        <div class="col-xs-8">
                                            <input class="form-control" placeholder="请输入订单编号" type="text" value="<%= DWYRequest.query.tid %>" name="tid">
                                        </div>
                                    </div>
                                </div>

                            </div>
                            <div class="col-xs-12">
                                <hr class="simple">
                                <div class="col-xs-12 text-center">
                                    <button class="btn btn-primary " type="submit">
                                        <i class="fa fa-search"></i>
                                        搜索
                                    </button>
                                </div>
                            </div>
                        </form>
                    </div>
                    <!-- end row -->
                </div>
            </div>
        </div>
        <!--S well-->
        <div class="well">
            <!-- row -->
            <div class="row">
                <div class="col-xs-12">
                    <legend>
                        待安装列表
                    </legend>
                </div>
            </div>
            <!-- row -->
            <div class="row">
                <div class="col-xs-12">
                    <div class="table-responsive">
                        <table class="table table-bordered table-striped">
                            <thead>
                            <tr>
                                <th>序号</th>
                                <th>订单编号</th>
                                <th>客户编号</th>
                                <th>客户名称</th>
                                <th>所属空间名称</th>
                                <th>父空间名称</th>
                                <th>产品名称</th>
                                <th>建单柜员编号</th>
                                <th>建单柜员名称</th>
                                <th>流水编号</th>
                                <th>操作</th>
                            </tr>
                            </thead>
                            <tbody>
                            <%
                            if(installServiceList.result){
                                installServiceList.result.forEach(function (element, index) { %>
                            <tr>
                                <td><%= index+1 %></td>
                                <td><a href="/order/detail/<%= element.tid %>"><%= element.tid %></a></td>
                                <td><%= element.cid %></td>
                                <td><%= element.name %></td>
                                <td><%= element.spaceName %></td>
                                <td><%= element.parentSpaceinfoName %></td>
                                <td><%= element.prodName %></td>
                                <td><%= element.gid %></td>
                                <td><%= element.gname %></td>
                                <td><%= element.lid %></td>
                                <td>
                                    <a class="label label-primary" id="installGroup">指定安装组</a>
                                </td>
                                <input type="hidden" value="<%= element.tid %>" name="oid">
                            </tr>
                            <%})}else{%>
                               <td colspan="12" class="text-align-center">暂无结果</td>
                           <% }%>
                            </tbody>
                        </table>
                        <%- pagination %>
                    </div>
                </div>
            </div>
        </div>
        <!--E well-->
    </div>
    <!--E content-->
</div>
<%- include('../layouts/footer'); %>
<%- include('../layouts/footer_common_script'); %>
<script type="text/javascript">
    $(document).ready(function () {
        pageSetUp();
        $(".fg-date-picker").flatpickr(data_time_picker.data_picker); // jQuery
        $("#installGroup").on(
            {
                click:function(){
                    layer.open({
                        title: false,
                        type: 1,
                        area: '700px',
                        content: $('#assignInstallGroup'),
                        cancel: function () {
                            //右上角关闭回调
                        },

                    });
                }
            }
        )
    })
</script>
<%- include('../layouts/foot'); %>
<%- include('./installService_modal'); %>