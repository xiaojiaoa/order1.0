<%- include('../layouts/head_top'); %>
<%- include('../layouts/head_bottom'); %>
<%- include('../layouts/header'); %>
<%- include('../layouts/nav'); %>


<!-- MAIN PANEL -->
<div id="main" role="main">

    <!-- RIBBON -->
    <div id="ribbon">

        <!-- breadcrumb -->
        <ol class="breadcrumb">
            <li>后台管理</li>
            <li>供应商信息</li>
        </ol>
        <!-- end breadcrumb -->
    </div>
    <!-- END RIBBON -->

    <!-- MAIN CONTENT -->
    <div id="content">

        <!-- well -->
        <div class="well">
            <!-- row -->
            <div class="row">
                <!-- col -->
                <div class="col-xs-12">
                    <legend>供应商信息</legend>
                    <div class="table-responsive">
                        <table class="table table-bordered table-striped table-information">
                            <tbody>
                            <tr>
                                <td>供应商分类编号</td>
                                <td colspan=""><%= suppliersDetail.cateId  %></td>
                                <td>供应商分类名称</td>
                                <td colspan=""><%= suppliersDetail.name  %></td>
                            </tr>
                            <tr>
                                <td>联系人姓名</td>
                                <td><%= suppliersDetail.contact  %></td>
                                <td>联系人手机</td>
                                <td><%= suppliersDetail.contactMobile  %></td>
                            </tr>
                            <tr>
                                <td>供应商所在地区</td>
                                <td colspan="3">
                                    <span class="margin-right-5-10"><%= suppliersDetail.province  %>,</span>
                                    <span class="margin-right-5-10"><%= suppliersDetail.city  %>,</span>
                                    <span class="margin-right-5-10"><%= suppliersDetail.dist  %>,</span>
                                </td>
                            </tr>
                            <tr>
                                <td>财务联系人</td>
                                <td colspan=""><%= suppliersDetail.financeContact  %></td>
                                <td>财务联系人手机</td>
                                <td colspan=""><%= suppliersDetail.financeContactMobile  %></td>
                            </tr>
                            <tr>
                                <td>开户银行</td>
                                <td><%= suppliersDetail.depositBank  %></td>
                                <td>开户卡号</td>
                                <td><%= suppliersDetail.depositNo  %></td>
                            </tr>
                            <tr>
                                <td>开票信息</td>
                                <td><%= suppliersDetail.invoiceInfo  %></td>
                                <td>详细地址</td>
                                <td colspan="3"><%= suppliersDetail.address  %></td>
                            </tr>
                            <tr>
                                <td>备注</td>
                                <td colspan="3"><%= suppliersDetail.remark  %></td>
                            </tr>
                            </tbody>
                        </table>

                    </div>

                </div>
                <!-- end col -->
                <div class="col-xs-8">
                    <hr class="simple">
                    <div class="col-xs-12 text-center" id="close-employee-btn">

                        <a class="btn btn-primary "
                           href="/supplier/modify">
                            <i class="fa fa-edit"></i>
                            修改供应商信息
                        </a>
                        <!--<a class="btn btn-primary" id="reset-password">-->
                            <!--<i class="fa fa-refresh"></i>-->
                            <!--密码重置-->
                        <!--</a>-->
                        <!--<a class="btn btn-warning" id="close-employee">-->
                            <!--<i class="fa fa-unlock-alt"></i>-->
                            <!--关闭账号-->
                        <!--</a>-->
                    </div>
                </div>
            </div>
            <!-- end row -->
        </div>
        <!-- end well -->


    </div>
    <!-- END MAIN CONTENT -->

</div>
<!-- END MAIN PANEL -->

<%- include('../layouts/footer'); %>
<%- include('../layouts/footer_common_script'); %>


<script type="text/javascript">

    $(document).ready(function () {


    })

</script>


<%- include('../layouts/foot'); %>