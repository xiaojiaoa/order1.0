<%- include('../layouts/head_top'); %>
<%- include('../layouts/head_bottom'); %>
<%- include('../layouts/header'); %>
<%- include('../layouts/nav'); %>


<!-- MAIN PANEL -->
<div id="main" role="main">

    <!-- RIBBON -->
    <div id="ribbon">

        <!-- breadcrumb -->
        <ol class="breadcrumb">
            <li>后台管理</li>
            <li>补单拆单管理页</li>
        </ol>
        <!-- end breadcrumb -->

    </div>
    <!-- END RIBBON -->


    <!-- MAIN CONTENT -->
    <div id="content">

        <div class="well">

            <!-- row -->
            <div class="row">
                <div class="col-xs-12">
                    <h3 class="text-align-center">拆单中</h3>
                    <hr class="simple">
                    <div class="table-responsive">
                        <table class="table table-bordered table-striped">
                            <thead>
                            <tr>
                                <th>序号</th>
                                <th>品牌</th>
                                <th>颜色</th>
                                <th>类别</th>
                                <th>难度</th>
                                <th>订单号</th>
                                <th>专卖店</th>
                                <th>客户名称</th>
                                <th>产品信息</th>
                                <th>受理人</th>
                                <th>收款确认日期</th>
                            </tr>
                            </thead>
                            <tbody>
                            <tr>
                                <td>1</td>
                                <td>布兰莎</td>
                                <td>苹果木</td>
                                <td>异型柜</td>
                                <td>二级</td>
                                <td><a href="/order/resupply/detail/11222">11222</a></td>
                                <td>浙江省杭州市湖滨银泰店</td>
                                <td><%= user.name %></td>
                                <td>客卧房隔断柜</td>
                                <td><%= user.name %></td>
                                <td>2016-10-10</td>
                            </tr>


                            </tbody>
                        </table>
                    </div>
                </div>
            </div>


        </div>




        <div class="well">

            <!-- row -->
            <div class="row">
                <!-- col -->
                <div class="col-xs-12">
                    <h3 class="text-align-center">已收款，未拆单</h3>
                    <hr class="simple">
                    <!--<legend>客户搜索</legend>-->
                    <div class="row search-form">
                        <form class="form-horizontal" method="get">

                            <div class="col-xs-12">

                                <div class="col-xs-7">
                                    <div class="form-group">
                                        <div class="col-xs-3">
                                            <select class="form-control" name="">
                                                <option value="">- 品牌 -</option>
                                            </select>
                                        </div>
                                        <div class="col-xs-3">
                                            <select class="form-control" name="">
                                                <option value="">- 颜色 -</option>
                                            </select>
                                        </div>
                                        <div class="col-xs-3">
                                            <select class="form-control" name="">
                                                <option value="">- 类型 -</option>
                                            </select>
                                        </div>

                                        <div class="col-xs-3">
                                            <select class="form-control" name="">
                                                <option value="">- 难度 -</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>

                                <div class="col-xs-4">
                                    <div class="form-group">
                                        <label class="pull-left control-label">&nbsp;收款日期</label>
                                        <div class="col-xs-4">
                                            <input class="form-control fg-date-picker" type="text"
                                                   value="" name="startDate">
                                        </div>
                                        <label class="control-label " style="float: left"> 至 </label>
                                        <div class="col-xs-4">
                                            <input class="form-control fg-date-picker" type="text"
                                                   value="" name="endDate">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-xs-1">
                                    <button class="btn btn-primary " type="submit">
                                        <i class="fa fa-search"></i>
                                        查询
                                    </button>
                                </div>


                            </div>
                        </form>
                    </div>
                </div>
                <!-- end col -->
            </div>

            <div class="row margin-top-10">
                <div class="table-responsive">
                    <table class="table table-bordered table-striped">
                        <thead>
                        <tr>
                            <th>序号</th>
                            <th>品牌</th>
                            <th>颜色</th>
                            <th>类别</th>
                            <th>难度</th>
                            <th>订单号</th>
                            <th>专卖店</th>
                            <th>客户名称</th>
                            <th>产品信息</th>
                            <th>受理人</th>
                            <th>收款确认日期</th>
                        </tr>
                        </thead>
                        <tbody>
                        <tr>
                            <td>1</td>
                            <td>布兰莎</td>
                            <td>苹果木</td>
                            <td>异型柜</td>
                            <td>二级</td>
                            <td><a href="/order/detail/555">11222</a></td>
                            <td>浙江省杭州市湖滨银泰店</td>
                            <td><%= user.name %></td>
                            <td>客卧房隔断柜</td>
                            <td><%= user.name %></td>
                            <td>2016-10-10</td>
                        </tr>


                        </tbody>
                    </table>
                </div>
            </div>
            <!-- end row -->
        </div>


    </div>
    <!-- end well -->


</div>
<!-- END MAIN CONTENT -->

</div>
<!-- END MAIN PANEL -->

<%- include('../layouts/footer'); %>
<%- include('../layouts/footer_common_script'); %>


