                                    <form class="form-horizontal" method="post" action="/orders/updateDifficultyLevel">
                                        <div class="col-xs-4">
                                            <select class="form-control" name="difficulty">
                                                <option value="">- 未定 -</option>
                                                <% difficultyList.forEach(function (element) { %>
                                                    <option value="<%= element.id %>" <%= orderInfo.difficulty == element.id ? 'selected': '' %>>
                                                        <%= element.name %>
                                                    </option>
                                                <% }) %>
                                            </select>
                                            <input value="<%= tid %>" name="tids" type="hidden">
                                        </div>
                                        <div class="col-xs-2">
                                            <% if (DWY_Helper.hasPermission(Permission.orderAssess.assess.id, permission) ){ %>
                                            <button class="btn btn-primary ">确定</button>
                                            <% } %>
                                        </div>
                                    </form>
                                    <% }else{  %>
                                        <%= DWY_Helper.getAssistVal(orderInfo.difficulty,difficultyList) %>
                                    <% } %>
                                </td>
                                <td>零售价</td>
                                <td><%= orderInfo.retailPrice %>元</td>
                            </tr>
                            <tr>
                                <td>结算价</td>
                                <td><%= orderInfo.revFranchisePrice %>元</td>
                                <td>出厂价</td>
                                <td><%= orderInfo.factoryPrice %>元</td>
                            </tr>

                            <tr>
                                <td>建单人</td>
                                <td><%= orderInfo.channelName %></td>
                                <td>设计师</td>
                                <td><%= orderInfo.designer %></td>
                            </tr>
                            <tr>
                                <td>建单时间</td>
                                <td colspan="3"><%= DWY_Helper.getLocalDate(orderInfo.addTime) %></td>
                            </tr>
                            <tr>
                                <td>备注</td>
                                <td colspan="3"><%= orderInfo.remark %></td>
                            </tr>
                            <tr>
                                <td>电商产品信息</td>
                                <td colspan="3" class="no-padding">
                                    <table class="table table-bordered table-striped table-noneinf">
                                        <thead>
                                        <tr>
                                            <th>ID</th>
                                            <th>型号</th>
                                            <th>名称</th>
                                            <th>尺寸</th>
                                            <th>参数</th>
                                            <th>单位</th>
                                            <th>数量</th>
                                            <th>说明</th>
                                            <th>编码</th>
                                        </tr>
                                        </thead>
                                        <tbody>
                                        <tr>
                                            <td>暂无数据</td>
                                            <td>暂无数据</td>
                                            <td>暂无数据</td>
                                            <td>暂无数据</td>
                                            <td>暂无数据</td>
                                            <td>暂无数据</td>
                                            <td>暂无数据</td>
                                            <td>暂无数据</td>
                                            <td>暂无数据</td>
                                        </tr>
                                        <tr>
                                            <td colspan="6" class="text-align-right">合计</td>
                                            <td colspan="3">暂无数据</td>
                                        </tr>
                                        </tbody>
                                    </table>
                                </td>
                            </tr>

                            <tr>
