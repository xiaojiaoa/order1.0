<%- include('../layouts/head_top'); %>
<%- include('../layouts/head_bottom'); %>
<%- include('../layouts/header'); %>
<%- include('../layouts/nav'); %>


<!-- MAIN PANEL -->
<div id="main" role="main">

    <!-- RIBBON -->
    <div id="ribbon">

        <!-- breadcrumb -->
        <ol class="breadcrumb">
            <li>后台管理</li>
            <li>订单详情</li>
        </ol>
        <!-- end breadcrumb -->
    </div>
    <!-- END RIBBON -->


    <!-- MAIN CONTENT -->
    <div id="content">


        <!-- well -->
        <div class="well">
            <!-- row -->
            <div class="row">
                <!-- col -->
                <div class="col-xs-12">
                    <legend>基本信息
                        <% if( orderInfo.stcode == 250 ){ %>
                        <a class="label label-primary" id="modifyInfo-modal">修改</a>
                        <% } %>
                    </legend>
                    <!-- row -->
                    <div class="table-responsive">
                        <table class="table table-bordered table-striped table-information">
                            <tbody>
                            <tr>
                                <td>订单状态</td>
                                <td><%= DWY_Helper.getAssistVal(orderInfo.stcode, stcodeInfo) %></td>
                                <td>订单号</td>
                                <td><%= tid %></td>
                            </tr>
                            <tr>
                                <td width="20%">产品信息</td>
                                <td width="30%"><%= orderInfo.prodInfo %></td>
                                <td width="20%">订单类型</td>
                                <td width="30%">普通订单</td>
                            </tr>
                            <tr>
                                <td>所在空间</td>
