                                    orderInfo.orders.forEach(function (orderElement) { %>
                                    <a class="order-number" href="/order/detail/<%= orderElement %>"><%= orderElement %></a>
                                    <% }) %>
                                </td>
                            </tr>
                            <tr>
                                <td>同客户流水</td>
                                <td colspan="3">
                                    <%
                                    var orderList = "";
                                    orderInfo.sequences.forEach(function (sequenceElement) { %>
                                    <a class="order-number"  href="/taskseq/index/<%= sequenceElement %>"><%= sequenceElement %></a>
                                    <% }) %>
                                </td>
                            </tr>
                            <tr>
                                <td>相关订单</td>
                                <td colspan="3">
                                    <%
                                    orderInfo.resupplys.forEach(function (element) { %>
                                    <a class="order-number" href="/order/resupply/detail/<%= element.tid %>/<%= element.resId %>"><%= element.tid %><%= element.resId %></a>
                                    <% }) %>
                                </td>
                            </tr>


                            </tbody>
                        </table>

                    </div>


                    <!-- end row -->
                </div>
                <!-- end col -->
            </div>

            <div class="row">
                <!-- col -->
                <div class="col-xs-12">
                    <legend>相关文件
                        <!--<a class="label label-primary" href="/file/create/<%= orderInfo.lid %>/<%= orderInfo.stcode %>">文件管理</a>-->

                        <% if( orderInfo.stcode == 250 || orderInfo.stcode == 450 || orderInfo.stcode == 475
                                || orderInfo.stcode == 550 || orderInfo.stcode == 650 || orderInfo.stcode == 635){ %>
                        <a class="label label-primary" href="/file/order/create/<%= orderInfo.lid %>/<%= orderInfo.stcode %>/1/<%= tid %>">文件管理</a>
                        <% } %>

                        <% if( orderInfo.stcode == 450 || orderInfo.stcode == 475
                                || orderInfo.stcode == 550 || orderInfo.stcode == 650 || orderInfo.stcode == 635 || (orderInfo.stcode>=675 && orderInfo.stcode<729 && orderInfo.packeted==1)){ %>
                        <a class="label label-primary" href="/order/workpiece/<%= tid %>">订单物料</a>
                        <% } %>

                    </legend>

                    <div class="table-responsive">
                        <table class="table table-bordered table-striped">
                            <thead>
                            <tr>
                                <th>序号</th>
                                <th>文件名</th>
                                <th>文件类型</th>
                                <!--<th>文件大小</th>-->
                                <th>上传人</th>
                                <th>上传时间</th>
                                <th>备注</th>
                            </tr>
                            </thead>
                            <tbody>
                            <%
                            orderInfo.ordfiles.forEach(function (element, index) { %>
                            <tr>
                                <td><%= index + 1 %></td>
                                <td>
                                    <a href="<%= DWY_GLOBAL.Static %>/download?url=<%= element.filePath %>"><%= element.fileName %></a>
                                </td>
                                <td><%= element.fileTypeDesc %></td>
                                <td><%= element.uploadName %></td>
                                <td><%= element.addTime %></td>
                                <td><%= element.remark %></td>
                            </tr>
                            <% }) %>
                            </tbody>
                        </table>

                    </div>


                    <!-- end row -->
                </div>
                <!-- end col -->
            </div>

            <% if(chgbackInfo.length>0){  %>
            <div class="row">
                <!-- col -->
                <div class="col-xs-12 ">
                    <legend>退回信息
