                            查看所有退回信息
                        </a>
                    </legend>

                    <div class="table-responsive">
                        <table class="table table-bordered table-striped">
                            <thead>
                            <tr>
                                <th width="100">序号</th>
                                <th>退回原因</th>
                                <th>退回类型</th>
                                <th>备注</th>
                                <th>操作柜员</th>
                            </tr>
                            </thead>
                            <tbody>
                            <%  chgbackInfo.forEach(function (element, index) {  %>
                            <tr>
                                <td><%= index+1 %></td>
                                <td><%= element.causeName %></td>
                                <td><%= element.reviewReasonTypeStr %></td>
                                <td><%= element.remark %></td>
                                <td><%= element.gname %></td>
                            </tr>
                            <%  })%>

                            </tbody>
                        </table>

                    </div>


                    <!-- end row -->
                </div>
                <!-- end col -->
            </div>

            <% } %>

            <div class="row">
                <!-- col -->
                <div class="col-xs-12">
