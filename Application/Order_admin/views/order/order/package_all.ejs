<%- include('../layouts/head_top'); %>
<%- include('../layouts/head_bottom'); %>
<%- include('../layouts/header'); %>
<%- include('../layouts/nav'); %>


<!-- MAIN PANEL -->
<div id="main" role="main" class="mainFlex">

    <!-- RIBBON -->
    <div id="ribbon">

        <!-- breadcrumb -->
        <ol class="breadcrumb">
            <li>后台管理</li>
            <li>订单包装</li>
        </ol>
        <!-- end breadcrumb -->

    </div>
    <!-- END RIBBON -->


    <!-- MAIN CONTENT -->
    <div id="content" class="dis-flex">
        <div class="well">
            <!-- row -->
            <div class="row">
                <!-- col -->
                <div class="col-xs-12">
                    <!--<legend>订单搜索</legend>-->
                    <div class="row search-form">
                        <form class="form-horizontal" method="get" id="toBePackedForm">

                            <div class="col-xs-12">

                                <!--输入字段-->
                                <div class="col-xs-3">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label">订单号:</label>
                                        <div class="col-xs-8">
                                            <input class="form-control" placeholder="请输入订单号" type="text" value="<%= DWYRequest.query.tid %>" name="tid">
                                        </div>
                                    </div>
                                </div>

                                <div class="col-xs-3">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label">机构号:</label>
                                        <div class="col-xs-8">
                                            <input class="form-control" placeholder="请输入批次号" type="text" value="<%= DWYRequest.query.bid %>" name="bid">
                                        </div>
                                    </div>
                                </div>
                                <div class="col-xs-3">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label">包装状态:</label>
                                        <div class="col-xs-8">
                                            <select class="form-control" name="packageStatus">
                                                <option value="">- 全部状态 -</option>
                                                <option value="1" <%= (DWYRequest.query.packageStatus ==1)?"selected":"" %>>未入库</option>
                                                <option value="2" <%= (DWYRequest.query.packageStatus ==2)?"selected":"" %>>已入库</option>


                                                <option value="3" <%= (DWYRequest.query.packageStatus ==3)?"selected":"" %>>已出库</option>

                                            </select>
                                        </div>
                                    </div>
                                </div>



                            </div>
                            <div class="col-xs-12">
                                <hr class="simple">
                                <div class="col-xs-12 text-center" >
                                    <button class="btn btn-primary " type="submit">
                                        <i class="fa fa-search"></i>
                                        查询
                                    </button>
                                </div>
                                <hr class="simple">
                            </div>

                        </form>
                    </div>
                    <!-- end row -->
                </div>
                <!-- end col -->
            </div>
            <!-- end row -->

        </div>

        <div class="well dis-flex">
            <!-- row -->
            <div class="row">
                <div class="col-xs-12">
                    <h6>待包装订单</h6>
                    <hr class="simple">
                </div>
            </div>
            <!--end roll-->
            <!-- row -->
            <div class="row dis-flex">
                <div class="col-xs-12 dis-flex">
                    <div class="table-responsive dis-flex">
                        <table class="table table-bordered table-striped table-hover table-scroll dis-flex">
                            <thead>
                            <tr>
                                <th width="4%">序号</th>
                                <th width="12%">订单号</th>
                                <th>包装流水号</th>
                                <th>包装序号</th>
                                <th>包装名称</th>
                                <th>包装类型</th>
                                <th>货位编号</th>
                            </tr>
                            </thead>
                            <tbody>
                            <%
                            if(DWY_Helper.isCanLoop(packageAll.result)){
                            packageAll.result.forEach(function (element, index) { %>
                            <tr>
                                <td width="4%"><%= index+1 %></td>
                                <td width="12%"><a href="/order/detail/<%= element.tid %>"><%= element.tid %></a></td>
                                <td><%= element.packageLid %></td>
                                <td><%= element.packageNo %></td>
                                <td><%= element.packageName %></td>
                                <td><%= element.packageTypeStr %></td>
                                <td><%= element.spaceId %></td>
                            </tr>
                            <%  })
                            }else{ %>
                            <td colspan="7" class="text-align-center">暂无结果</td>
                            <%  }%>
                            </tbody>
                        </table>
                    </div>
                    <%- pagination %>
                </div>
            </div>
            <!--end  row-->
        </div>
        <!--end well-->
      </div>



</div>
<!-- END MAIN CONTENT -->

</div>
<!-- END MAIN PANEL -->

<%- include('../layouts/footer'); %>
<%- include('../layouts/footer_common_script'); %>
<script type="text/javascript">

    $(document).ready(function () {
        pageSetUp();

        //对表单进行初始化验证
        $("#toBePackedForm").validationEngine();

    })

</script>
<%- include('../layouts/foot'); %>