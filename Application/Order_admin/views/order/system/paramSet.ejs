<%- include('../layouts/head_top'); %>
<%- include('../layouts/head_bottom'); %>
<%- include('../layouts/header'); %>
<%- include('../layouts/nav'); %>


<!-- MAIN PANEL -->
<div id="main" role="main">

    <!-- RIBBON -->
    <div id="ribbon">

        <!-- breadcrumb -->
        <ol class="breadcrumb">
            <li>后台管理</li>
            <li><a class="back-url" href="/system">系数配置</a></li>
            <li>价格系数配置</li>
            <li><a class="back-url" href="javascript:history.go(-1)"><i class="fa fa-chevron-left"></i> 返回</a></li>
        </ol>
        <!-- end breadcrumb -->


    </div>
    <!-- END RIBBON -->


    <!-- MAIN CONTENT -->
    <div id="content">


        <!-- well -->
        <div class="well">
            <!-- row -->
            <div class="row">
                <!-- col -->
                <div class="col-xs-12">
                    <legend>价格系数配置</legend>


                    <!-- row -->
                    <div class="row search-form">
                        <form class="form-horizontal" method="post" action="/param/doParam" id="create-customer">

                            <div class="col-xs-12">

                                <!--键值+下拉选择+输入组合-->
                                <div class="col-xs-4">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label ">结算价系数: </label>
                                        <div class="col-xs-8">
                                            <input class="form-control validate[custom[number],maxSize[4]]"
                                                   value="<%= paramInfo.purchaseCoefficient %>" name="purchaseCoefficient">
                                            <span code-data="" class="unit"></span>
                                        </div>

                                    </div>
                                </div>
                                <div class="col-xs-4">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label ">零售价系数: </label>
                                        <div class="col-xs-8">
                                            <input class="form-control validate[custom[number],maxSize[4]]"
                                                   value="<%= paramInfo.retailCoefficient %>" name="retailCoefficient">
                                            <span code-data="" class="unit"></span>
                                        </div>

                                    </div>
                                </div>
                            </div>
                            <input type="hidden" name="id" value="<%= id%>"/>

                            <div class="col-xs-8">
                                <hr class="simple">
                                <div class="col-xs-12 text-center">

                                    <button class="btn btn-primary " type="submit">
                                        <i class="fa fa-save"></i>
                                        确认添加
                                    </button>
                                </div>
                            </div>

                        </form>
                    </div>
                    <!-- end row -->

                </div>
                <!-- end col -->
            </div>
            <!-- end row -->
        </div>
        <!-- end well -->


    </div>
    <!-- END MAIN CONTENT -->

</div>
<!-- END MAIN PANEL -->

<%- include('../layouts/footer'); %>
<%- include('../layouts/footer_common_script'); %>


<script type="text/javascript">

    $(document).ready(function () {



        pageSetUp();

        $("#create-customer").validationEngine();


    })

</script>


<%- include('../layouts/foot'); %>