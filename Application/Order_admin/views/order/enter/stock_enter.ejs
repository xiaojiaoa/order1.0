<%- include('../layouts/head_top'); %>
<%- include('../layouts/head_bottom'); %>
<%- include('../layouts/header'); %>
<%- include('../layouts/nav'); %>


<!-- MAIN PANEL -->
<div id="main" role="main">

    <!-- RIBBON -->
    <div id="ribbon">

        <!-- breadcrumb -->
        <ol class="breadcrumb">
            <li>后台管理</li>
            <li><a class="back-url" href="/enterMaterial"><i class="fa fa-chevron-left"></i> 原料入库</a></li>
            <li><a class="back-url" href="/enterMaterial/stockOver"><i class="fa fa-chevron-left"></i> 采购完成单</a></li>
            <li>入库</li>
        </ol>


    </div>
    <!-- END RIBBON -->


    <!-- MAIN CONTENT -->
    <div id="content">
        <!-- well -->
        <div class="well">
            <!-- row -->
            <div class="row">
                <!-- col -->
                <div class="col-xs-12">
                    <!--<legend>客户搜索</legend>-->
                    <!-- row -->
                    <div class="row search-form">
                        <form class="form-horizontal" method="get" id="search-customer">
                            <div class="col-xs-12">

                                <div class="col-xs-3">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label">仓库:</label>
                                        <div class="col-xs-8">
                                            <select class="form-control" name="whseId">
                                                <option value="">- 请选择 -</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                                <div class="col-xs-3">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label">区域:</label>
                                        <div class="col-xs-8">
                                            <select class="form-control" name="regionId">
                                                <option value="">- 请选择 -</option>
                                            </select>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-xs-12">
                                <hr class="simple">
                                <div class="col-xs-12 text-center">
                                    <button class="btn btn-primary " type="submit">
                                        <i class="fa fa-search"></i>
                                        查询
                                    </button>
                                </div>
                            </div>

                        </form>
                    </div>
                    <!-- end row -->
                </div>
                <!-- end col -->
            </div>
            <!-- end row -->
        </div>
        <!-- end well -->

        <div class="well">
            <div class="row">
                <form class="form-horizontal" method="post" id="check_pass" action="/enterMaterial/stockOver/toEnter/doEnter">
                    <div class="col-xs-12">
                        <div class="table-responsive">
                            <table class="table table-bordered table-striped table-hover table-editinput">
                                <thead>
                                <tr>
                                    <th>序号</th>
                                    <th>请购单号</th>
                                    <th>采购单号</th>
                                    <th>物料编号</th>
                                    <th>物料名称</th>
                                    <th>单价</th>
                                    <th>采购数量</th>
                                    <th>采购金额</th>
                                    <th width="200">货位号</th>
                                    <th width="100">入库数量</th>
                                </tr>
                                </thead>
                                <tbody>
                                <% if(result.length>0){%>
                                    <% result.forEach(function(element,index){%>
                                        <tr>
                                            <td><%=++index%></td>
                                            <td><%=element.reqId%></td>
                                            <td><a href="/purchase/orderDetail/<%=element.orderId%>"><%=element.orderId%></a></td>
                                            <td><%=element.mateId%></td>
                                            <td><%=element.mateName%></td>
                                            <td><%=element.price%></td>
                                            <td><%=element.amount%></td>
                                            <td><%=element.totalAmount%></td>
                                            <td><input class="form-control long" value="" name="huowei0"></td>
                                            <td><input class="form-control short" value="8" name="num0"></td>
                                        </tr>
                                <%})}else{%>
                                    <tr>
                                        <td colspan="9" class="text-align-center">暂无数据</td>
                                    </tr>
                                <%}%>
                                </tbody>
                            </table>

                        </div>
                    </div>
                    <div class="col-xs-12">
                        <hr class="simple">
                        <div class="col-xs-12 text-center">
                            <a class="btn btn-primary" id="addMater">
                                <i class="fa fa-plus"></i>
                                添加入库物料
                            </a>
                            <button class="btn btn-primary " type="submit">确认入库</button>
                        </div>
                    </div>
                </form>
            </div>
        </div>
    </div>
    <!-- END MAIN CONTENT -->

</div>
<!-- END MAIN PANEL -->

<%- include('../layouts/footer'); %>
<%- include('../layouts/footer_common_script'); %>


<script type="text/javascript">

    $(document).ready(function () {

        $("#search-customer").validationEngine();

        pageSetUp();

    })

</script>


<%- include('./add_materials'); %>
<%- include('../layouts/foot'); %>