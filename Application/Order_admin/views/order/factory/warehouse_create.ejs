<%- include('../layouts/head_top'); %>
<%- include('../layouts/head_bottom'); %>
<%- include('../layouts/header'); %>
<%- include('../layouts/nav'); %>


<!-- MAIN PANEL -->
<div id="main" role="main">

    <!-- RIBBON -->
    <div id="ribbon">

        <!-- breadcrumb -->
        <ol class="breadcrumb">
            <li>后台管理</li>
            <li><a class="back-url" href="/factory"><i class="fa fa-chevron-left"></i> 工厂列表</a></li>
            <li><a class="back-url" href="/warehouse?ftyId=<%= ftyId %>"><i class="fa fa-chevron-left"></i> 仓库列表</a></li>
            <li>新建仓库</li>
        </ol>
        <!-- end breadcrumb -->
    </div>
    <!-- END RIBBON -->

    <!-- MAIN CONTENT -->
    <div id="content">

        <!-- well -->
        <div class="well">
            <!-- row -->
            <div class="row">
                <!-- col -->
                <div class="col-xs-12">
                    <legend>新建仓库信息</legend>

                    <!-- row -->
                    <div class="row">
                        <form class="form-horizontal" method="post" action="/warehouse/doCreate" id="create-store">

                            <div class="col-xs-12">

                                <!--输入字段-->
                                <div class="col-xs-8">
                                    <div class="form-group">
                                        <label class="col-xs-2 control-label"><span class="requird"></span>
                                            所属工厂:</label>
                                        <div class="col-xs-10">
                                            <select class="form-control validate[required]" name="ftyIdselect" disabled>
                                                <option value="">- 请选择 -</option>
                                                <% factoryList.forEach(function (element) {  %>
                                                <option value="<%= element.ftyId %>"  <%= (ftyId == element.ftyId )?"selected":"" %> >
                                                    <%= element.name %>
                                                </option>
                                                <% }) %>
                                            </select>
                                            <input name="ftyId" value="<%= ftyId %>" type="hidden">
                                        </div>
                                    </div>
                                </div>

                            </div>

                            <div class="col-xs-12">

                                <div class="col-xs-4">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label "><span class="requird"></span> 仓库名称:
                                        </label>
                                        <div class="col-xs-8">
                                            <input class="form-control validate[required]"
                                                   value="<%= DWY_last_request_param.name %>"
                                                   name="name">

                                        </div>

                                    </div>
                                </div>

                                <!--<div class="col-xs-4">-->
                                    <!--<div class="form-group">-->
                                        <!--<label class="col-xs-4 control-label "><span class="requird"></span> 仓库类型:</label>-->
                                        <!--<div class="col-xs-8">-->
                                            <!--<select class="form-control validate[required]" name="type">-->
                                                <!--<option value="">- 请选择 -</option>-->
                                                <!--<% warehouseTypes.forEach(function (element) {  %>-->
                                                <!--<option value="<%= element.id %>" <%= DWY_last_request_param.type == element.id ?"selected":"" %> >-->
                                                    <!--<%= element.name %>-->
                                                <!--</option>-->
                                                <!--<% }) %>-->
                                            <!--</select>-->
                                        <!--</div>-->
                                    <!--</div>-->
                                <!--</div>-->
                            </div>


                            <div class="col-xs-12">

                                <!--输入字段-->
                                <div class="col-xs-4">
                                    <div class="form-group">
                                        <label class="col-xs-4 control-label"><span class="requird"></span> 编号:</label>
                                        <div class="col-xs-8">
                                            <div class="input-group">
                                                <span class="input-group-addon"><%= ftyId %></span>
                                                <input class="form-control validate[required,custom[onlyNumberSp],min[10],max[99]]"
                                                       value="<%= DWY_last_request_param.whseId %>"
                                                       placeholder="请输入2位仓库编码"
                                                       name="whseId">
                                            </div>

                                        </div>
                                    </div>
                                </div>

                            </div>


                            <div class="col-xs-12">
                                <div class="col-xs-8">
                                    <div class="form-group">
                                        <label class="col-xs-2 control-label">备注:</label>
                                        <div class="col-xs-10">
                                            <textarea name="remark" class="form-control"
                                                      cols="30" rows="5"><%= DWY_last_request_param.remark %></textarea>
                                        </div>
                                    </div>
                                </div>
                            </div>

                            <div class="col-xs-8">
                                <hr class="simple">
                                <div class="col-xs-12 text-center">

                                    <button class="btn btn-primary " type="submit">
                                        <i class="fa fa-save"></i>
                                        确认添加
                                    </button>
                                </div>
                            </div>

                        </form>
                    </div>
                    <!-- end row -->

                </div>
                <!-- end col -->
            </div>
            <!-- end row -->
        </div>
        <!-- end well -->


    </div>
    <!-- END MAIN CONTENT -->

</div>
<!-- END MAIN PANEL -->

<%- include('../layouts/footer'); %>
<%- include('../layouts/footer_common_script'); %>


<script type="text/javascript">

    $(document).ready(function () {

//        pageSetUp();

        $("#create-store").validationEngine();

    })

</script>


<%- include('../layouts/foot'); %>