                                <th>供应商ID</th>
                                <th>厚度</th>
                                <th>密度</th>
                                <th>发生日期</th>
                                <th>数量</th>
                                <th>事项</th>
                                <th>经手</th>
                                <th>单号</th>
                                <th>备注</th>
                            </tr>
                            </thead>
                            <tbody>

                            <tr>
