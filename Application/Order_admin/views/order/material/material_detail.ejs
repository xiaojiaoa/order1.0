                                <td>车间领料</td>
                                <td>张三</td>
                                <td>LL1610101234</td>
                                <td></td>
                            </tr>

                            </tbody>
                        </table>
                                        </div>
                </div>
            </div>
        </div>
        <!--end well-->
    </div>
    <!-- END MAIN CONTENT -->

</div>
<!-- END MAIN PANEL -->

